{
  "manifest_version": 3,
  "name": "DuetLingo",
  "version": "1.0",
  "description": "Instant Language Swap Extension",
  "permissions": ["scripting", "activeTab", "storage"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "action": {
    "default_popup": "popup.html"
  }
}
